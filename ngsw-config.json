{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],"dataGroups": [
    {
      "name": "api-call",
      "urls": ["https://coronavirus-19-api.herokuapp.com/countries/TUNISIA","https://covid19.soficoop.com/country/tn","https://corona.lmao.ninja/v2/countries"],
      "cacheConfig": {
        "maxAge": "12h",
        "maxSize": 100,
        "strategy": "freshness",
        "timeout": "5s"
      }
    }
  ]
}
